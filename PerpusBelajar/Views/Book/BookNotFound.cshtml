@model int

@{
    ViewData["Title"] = "404 Error";
}

<div class="alert alert-danger mt-1 mb-1">
    <h4>404 Not Found Error: </h4>
    <hr />
    <h5>
        Book with Id = @Model cannot be found 
    </h5>
</div>
<a asp-action="index" asp-controller="Book" class="btn btn-success" style="width:auto;">
    Click here to see books list
</a>


